{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "trybe-frontend",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ]
}
